  "components": {
    "schemas": {
      "AppUserLogin": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CreateAppUser": {
        "required": [
          "email",
          "firstname",
          "lastname",
          "password",
          "role",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "maxLength": 12,
            "minLength": 3,
            "type": "string"
          },
          "password": {
            "maxLength": 24,
            "minLength": 8,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "firstname": {
            "minLength": 1,
            "type": "string"
          },
          "lastname": {
            "minLength": 1,
            "type": "string"
          },
          "specialization": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "maxLength": 350,
            "type": "string",
            "nullable": true
          },
          "role": {
            "$ref": "#/components/schemas/Role"
          }
        },
        "additionalProperties": false
      },
      "CreateAppointment": {
        "required": [
          "description",
          "endTime",
          "expertId",
          "startTime",
          "userId"
        ],
        "type": "object",
        "properties": {
          "description": {
            "maxLength": 250,
            "minLength": 0,
            "type": "string"
          },
          "startTime": {
            "type": "string",
            "format": "date-time"
          },
          "endTime": {
            "type": "string",
            "format": "date-time"
          },
          "expertId": {
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Role": {
        "enum": [
          0,
          1
        ],
        "type": "integer",
        "format": "int32"
      },
      "Status": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "UpdateAppUser": {
        "required": [
          "email",
          "firstname",
          "lastname"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "firstname": {
            "minLength": 1,
            "type": "string"
          },
          "lastname": {
            "minLength": 1,
            "type": "string"
          },
          "specialization": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "maxLength": 350,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UpdateAppointment": {
        "required": [
          "description",
          "endTime",
          "expertId",
          "startTime",
          "status",
          "userId"
        ],
        "type": "object",
        "properties": {
          "description": {
            "maxLength": 250,
            "minLength": 0,
            "type": "string"
          },
          "startTime": {
            "type": "string",
            "format": "date-time"
          },
          "endTime": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "$ref": "#/components/schemas/Status"
          },
          "expertId": {
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Authorization header using the Bearer scheme. Example: \"Bearer {token}\"",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
